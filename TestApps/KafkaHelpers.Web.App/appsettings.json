{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "KafkaHelpers": {
    "Configs": {
      "Global": {
        "bootstrap.servers": "localhost:9092"
      },
      "Defaults": {
        "Producer": {
          "batch.size": 1
        }
      },
      "StandardProducerConfig": {
        "bootstrap.servers": "localhost:9092"
      },
      "Producers": {
        "TestProducer": {
          "acks": "all"
        }
      }
    }
  }
}
